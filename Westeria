local Toggle1 = Section1:CreateToggle("Kill Aura", nil, function(State)
sex = State
while sex do
    wait(0.5)
    game:GetService("Players").LocalPlayer.Character.Scripts.Sheath:FireServer(true)
    for i, v in ipairs(game:GetService("Workspace").Living:GetChildren()) do
        if
            v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Name ~= game.Players.LocalPlayer.Name and
                v.Name ~= "cegberry"
         then
            local Root = game.Players.LocalPlayer.Character.HumanoidRootPart
            local RootTwo = v.HumanoidRootPart
            if (Root.Position - RootTwo.Position).Magnitude < 50 then
                game:GetService("ReplicatedStorage").Events.Execute:FireServer(
                    {v},
                    game:GetService("ReplicatedStorage").Styles.Katana.Vulnerable,
                    69420,
                    69420
                )
            end
        end
    end
end
end)

