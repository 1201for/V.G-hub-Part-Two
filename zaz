local OldNameCall = nil
OldNameCall =
    hookmetamethod(
    game,
    "__namecall",
    function(self, ...)
        local Args = {...}
    
        if getnamecallmethod() == "BreakJoints" then
            return true
        end
        if getnamecallmethod() == "Destroy" and self == game.Players.LocalPlayer.Character then 
            return nil
        end 
        return OldNameCall(self, unpack(Args))
    end
)
