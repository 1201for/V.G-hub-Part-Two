local Toggle1 = Section1:CreateToggle("Anti Sun", nil, function(State)
falls = State
local OldNamecall
OldNamecall =
    hookmetamethod(game,"__namecall",function(self, ...)
        local shit = {...}
        if getnamecallmethod() == "FireServer" and shit[2] == "DemonWeakness" and falls
         then
            return wait(math.huge)
        end
        return OldNamecall(self, ...)
    end
)
end)

