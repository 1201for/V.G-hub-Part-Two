local OldNameCall = nil
OldNameCall = hookmetamethod(game, "__namecall", function(v, ...)
    local Args = {...}
    if not checkcaller() and v == "WalkSpeed" or v == "JumpPower" then
            return 
    end
    return OldNameCall(v, ...)
end)


local function getNearestTricket()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(game:GetService("Workspace"):GetChildren()) do
        if v:FindFirstChild("Part")  and v:FindFirstChild("Id") then
            local Mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v:FindFirstChild("Part").Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end;

local function getNearestSlayer()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(game:GetService("Workspace"):GetChildren()) do
        if v.Name:match("Slayer") and v:FindFirstChild("HumanoidRootPart")  then
            local Mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end;

local function getNearestDemon()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(game:GetService("Workspace"):GetChildren()) do
        if v:FindFirstChild("Demon") and v:FindFirstChild("HumanoidRootPart")  then
            local Mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end;

local function getNearestDrop()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(game:GetService("Workspace"):GetChildren()) do
        if v.Name == "DropItem" then
            local Mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).Magnitude 
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end;

local function Tween(A, B, C)
    if A and B then
        local Time = (B - A.Position).magnitude/C
        local Info = TweenInfo.new(Time, Enum.EasingStyle.Linear)
        local Tween = game:GetService("TweenService"):Create(A,Info,{CFrame = CFrame.new(B.X,B.Y,B.Z)})
        Tween:Play()
        wait(Time)
    end
end

getgenv().Tween = 50
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/1201for/V.G-Hub/main/im-retarded-3"))()
local Window = Library:CreateWindow(Config, game:GetService("CoreGui"))

