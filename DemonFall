        local shit = {...}
        if getnamecallmethod() == "FireServer"  and shit[2] == "FallDamageServer" and fall
         then
            return nil
        end
        return OldNamecall(self, ...)
    end
)
end)

local Toggle1 = Section1:CreateToggle("Tricket Esp", nil, function(State)

getgenv().esp = State
local function esp(v)
    local T = Drawing.new("Text")
    T.Visible = false
    T.Center = true
    T.Outline = true
    T.Font = 2
    T.Text = "Tricket"
    T.Size = 14
    T.Color = Color3.fromRGB(0, 255, 0)
    local function update()
        local Re
        Re =
            game:GetService('RunService').Stepped:Connect(
            function()
                if v and v:FindFirstChild("Spawned") then
                    local Vector, Screen = workspace.CurrentCamera:WorldToViewportPoint(v.Position)
                    if getgenv().esp and Screen then
                        T.Position = Vector2.new(Vector.X, Vector.Y)
                        T.Visible = true
                        T.Text = "Tricket"
                    else
                        T.Visible = false
                    end
                elseif v:FindFirstChild("Spawned") == nil then
                    T:Remove()
                    Re:disconnect()
                end
            end
        )
    end
    update()
end
for i, v in pairs(workspace.Trinkets:GetChildren()) do
    if v:IsA("Part") and v:FindFirstChild("Spawned") then
        esp(v)
    end
end
workspace.Trinkets.DescendantAdded:Connect(
    function(v)
        if v:IsA("IntValue") and string.match(v.Name, "Spawned") then
            esp(v.Parent)
        end
    end
)

end)

