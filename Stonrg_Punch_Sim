punch = State
game:GetService('RunService').Stepped:connect(function()
if punch then
game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"Activate_Punch"})
end end)
end)

local Toggle1 = Section1:CreateToggle("Auto Orbs", nil, function(State) 
wow = State
while wow do wait()
